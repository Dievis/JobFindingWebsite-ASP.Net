@model DACS.Models.PhieuDangTuyen

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}
<div class="container mt-5">
    <h1>Xóa thông tin phiếu đăng tuyển</h1>
    <h3>Bạn có chắc chắn muốn xóa thông tin này không?</h3>
    <div>
        <h4>PhieuDangTuyen</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">
                Nhà tuyển dụng:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NhaTuyenDungs.TenNTD)
            </dd>
            <dt class="col-sm-2">
                Tên ngành:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TenNganh)
            </dd>
            <dt class="col-sm-2">
                Mức lương:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MucLuong)
            </dd>
            <dt class="col-sm-2">
                Tỉnh:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Tinhs.TenTinh)
            </dd>
            <dt class="col-sm-2">
                Tên việc làm:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.TenViecLam)
            </dd>
            <dt class="col-sm-2">
                Chức danh:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ChucDanh)
            </dd>
            <dt class="col-sm-2">
                Mức lương:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MucLuong)
            </dd>
            @* <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.MoTa)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.MoTa)
            </dd> *@
            <dt class="col-sm-2">
                Địa điểm
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DiaDiem)
            </dd>
            <dt class="col-sm-2">
                Ngày lập phiếu:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.NgayLapPhieu)
            </dd>
            <dt class="col-sm-2">
                Hạn nộp:
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.HanNopHoSo)
            </dd>
            @* <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Statuses)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Statuses.StatusName)
            </dd> *@
        </dl>

        <form asp-action="Delete">
            <input type="hidden" asp-for="Id" />
            <input type="submit" value="Xóa" class="btn btn-danger" /> |
            <a class="btn btn-secondary" asp-action="Index">Quay lại danh sách</a>
        </form>
    </div>
</div>

